@article{ ISI:A1993LZ30000007,
Author = {LEPATOUREL, GNJ},
Title = {{COLD-TOLERANCE OF THE ORIENTAL COCKROACH BLATTA-ORIENTALIS}},
Journal = {{ENTOMOLOGIA EXPERIMENTALIS ET APPLICATA}},
Year = {{1993}},
Volume = {{68}},
Number = {{3}},
Pages = {{257-263}},
Month = {{SEP}},
Abstract = {{The cold-hardiness of nymphal stages 1 to 5 and adult male and female
   Blatta orientalis was tested at 2-degrees, -5-degrees and -10-degrees-C.
   The LT50 (time) of insects exposed to -5-degrees ranged from 0.21 to
   0.43 days. Acclimation at 10-degrees-C for times varying up to 14 days
   progressively increased cold-hardiness. A 14 day acclimation at
   10-degrees prior to exposure at -5-degrees-C increased LT50 (time) to
   1.1-4.2 days for the various stages; prolonging the acclimation time to
   28 days produced no further increase in LT50. All stages were rapidly
   killed at -10-degrees-C (LT50 < 0.04 days) and survived prolonged
   exposures at 2-degrees-C (LT50s from 16 to > 42 days) following
   acclimation. The potential for survival of outdoor populations of B.
   orientalis over winter is discussed.}},
