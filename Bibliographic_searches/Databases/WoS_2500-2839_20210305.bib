@article{ ISI:A1994PR32100004,
Author = {VANDERLINGEN, CD},
Title = {{ASPECTS OF THE EARLY-LIFE HISTORY OF GALJOEN DICHISTIUS-CAPENSIS}},
Journal = {{SOUTH AFRICAN JOURNAL OF MARINE SCIENCE-SUID-AFRIKAANSE TYDSKRIF VIR
   SEEWETENSKAP}},
Year = {{1994}},
Volume = {{14}},
Pages = {{37-45}},
Abstract = {{Laboratory experiments were conducted to determine the relationship
   between temperature and embryonic duration for fertilized galjoen eggs,
   and to examine the pattern of endogenous food utilization in early
   galjoen larvae.  Embryonic duration decreased with increasing
   temperature, the relationship being best described by a power function
   with a negative slope.  The lower lethal limit for developing eggs lay
   between 14 and 16-degrees-C.  Newly hatched larvae rapidly exhaust their
   endogenous food reserves, the yolk sac being depleted at a slightly
   faster rate than the oil globule.  Larvae approach first-feeding at 110
   h after hatching at 18-degrees-C, and exhibit a short time period to
   irreversible starvation.  Galjoen larvae appear to be particularly
   vulnerable to catastrophic starvation-induced mortality in nature.}},
