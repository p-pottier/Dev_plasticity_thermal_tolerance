@article{ ISI:A1991GL94100005,
Author = {DAINTON, BH},
Title = {{HEAT TOLERANCE AND THYROID-ACTIVITY IN DEVELOPING TADPOLES AND JUVENILE
   ADULTS OF XENOPUS-LAEVIS (DAUDIN)}},
Journal = {{JOURNAL OF THERMAL BIOLOGY}},
Year = {{1991}},
Volume = {{16}},
Number = {{5}},
Pages = {{273-276}},
Month = {{SEP}},
Abstract = {{1. Tolerance of short exposure to high temperatures in Xenopus laevis
   tadpoles decreased as metamorphosis proceeded and reached its lowest
   value at the metamorphic climax. This decrease may be correlated with
   the increasing activity of the thyroid gland.
   2. Following the metamorphic climax, tolerance of high temperatures
   increased when the activity of the thyroid gland decreased.
   3. In juvenile adults the level of thyroid activity, as measured by the
   uptake of I-131 and the histological appearance of the gland, was found
   to vary inversely with the ambient temperature.}},
