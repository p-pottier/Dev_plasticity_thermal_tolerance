@article{ ISI:A1991FC68500007,
Author = {MATSUURA, H and NAITO, A},
Title = {{STUDIES ON THE COLD-HARDINESS AND OVERWINTERING OF SPODOPTERA-LITURA F
   (LEPIDOPTERA, NOCTUIDAE) .2. THE LOWER LETHAL TEMPERATURE}},
Journal = {{JAPANESE JOURNAL OF APPLIED ENTOMOLOGY AND ZOOLOGY}},
Year = {{1991}},
Volume = {{35}},
Number = {{1}},
Pages = {{45-48}},
Month = {{FEB}},
Abstract = {{Cold-hardiness of S. litura larvae and pupae surviving in the late
   autumn or in the early winter was examined.  The mean supercooling point
   of the larvae was about-8-degrees-C.  However, -5-degrees-C was a
   critical point for the larvae:  the percentage of larvae frozen to death
   by exposure to -5-degrees-C for 12 hr, 24 hr and 48 hr were 0, 24 and
   100 respectively.  All larvae were susceptible to freezing and died
   soon.  No dead larvae were observed when subjected to 0-degrees-C even
   for 48 hr.  Supercooling points of pupae were at around -9-degrees-C,
   for one group and at around - 16-degrees-C, for another group.  The
   former was younger pupae; the latter was older ones.  Over 80\% of pupae
   did not freeze when subjected to -5-degrees-C for 48 hr, but died while
   being reared at under 20-degrees-C.  When the larvae were placed into
   wet conditions such as moist soil, freezing occurred at a higher
   temperature level than the mean supercooling point.  This was due to ice
   inoculation.}},
