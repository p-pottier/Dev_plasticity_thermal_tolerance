@article{ ISI:A1993MB57300007,
Author = {STRANGE, RJ and PETRIE, RB and CECH, JJ},
Title = {{SLIGHT STRESS DOES NOT LOWER CRITICAL THERMAL MAXIMUMS IN
   HATCHERY-REARED RAINBOW-TROUT}},
Journal = {{FOLIA ZOOLOGICA}},
Year = {{1993}},
Volume = {{42}},
Number = {{3}},
Pages = {{251-256}},
Abstract = {{This study evaluated the physiological response of rainbow trout to
   various initial stressors, either confinement or electroshock, followed
   by a heat challenge to determine how the critical thermal maximum of the
   fish was affected by the initial stressor. Plasma cortisol and glucose
   levels were determined following the initial stressor and the challenge
   stressor. The initial stressor induced a mild, typical, response by
   increasing cortisol levels to 112 ng/mL for the 60-minute confinement
   group and 68 ng/mL for the electroshocked group. The challenge stressor
   did not induce any further cortisol increase in fish receiving an
   initial stress, and in fact, cortisol levels decreased significantly (P
   < .05) after the heat challenge in groups that had prior confinement.
   There was only a narrow range (29.4-30.0-degrees-C) of critical thermal
   maximums among the treatment groups. The critical thermal maximum of the
   electroshocked-group (30.0-degrees-C), however, was significantly higher
   than the other groups.}},
