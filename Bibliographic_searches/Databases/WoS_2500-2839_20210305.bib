@article{ ISI:A1992KF31500006,
Author = {MAIER, G},
Title = {{DEVELOPMENT, REPRODUCTION AND GROWTH-PATTERN OF 2 COEXISTING, POND
   DWELLING CLADOCERANS}},
Journal = {{INTERNATIONALE REVUE DER GESAMTEN HYDROBIOLOGIE}},
Year = {{1992}},
Volume = {{77}},
Number = {{4}},
Pages = {{621-632}},
Abstract = {{Laboratory investigations on life history paramaters of 2 coexisting
   cladocerans (Daphnia obtusa, Moina brachiata) from a nearly temporary
   pond in South Germany revealed that the species have different
   temperature tolerances and temperature optima. D. obtusa experienced the
   highest reproductive success at 15 and 20-degrees-C and was able to
   survive and to reproduce at 2-degrees-C but died at 30-degrees-C. The
   reproductive success of M. brachiata was highest at 25 and 30-degrees-C
   and the species could not withstand temperatures < 15-degrees-C and
   greater-than-or-equal-to 35-degrees-C. At temperatures between
   approximately 20 and 25-degrees-C. where both cladocerans coexisted in
   nature, M. brachiata showed a faster population growth due to its
   approximately twofold higher egg production rates (10-12 eggs female-1
   day-1 compared to approximately 5 eggs female-1 day-1 in D.obtusa) and
   its shorter juvenile development (3.3 and 2.4 days compared to 6.3 and
   5.3 days in D. obtusa) M. brachiata needs generally only 3 molts to
   reach maturity while D. obtusa requires 5-6 molts.}},
