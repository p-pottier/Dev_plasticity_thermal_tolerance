@article{ ISI:A1994PE69800004,
Author = {CHAUVIN, G and VANNIER, G},
Title = {{INSECTICIDAL PROPERTIES OF PARADICHLOROBENZENE AND CAMPHOR - EFFECTS ON
   BEHAVIOR, TRANSPIRATION AND HEAT-RESISTANCE - A PRELIMINARY-STUDY ON
   TINEOLA-BISSELLIELLA HUM, (LEPIDOPTERA, TINEIDAE)}},
Journal = {{ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY}},
Year = {{1994}},
Volume = {{15}},
Number = {{1}},
Pages = {{23-29}},
Note = {{1st European Workshop on Invertebrate Ecophysiology, PAIMPONT, FRANCE,
   SEP 21-25, 1992}},
Organization = {{UNIV RENNES I; CNRS; SOC ECOPHYSIOL}},
Abstract = {{In a confined atmosphere, vapours of paradichlorobenzene are toxic to
   adults an Tineola bisselliella. Adults die after 30 hours of exposure to
   vapours; thirty-five days old larvae stop feeding and die after about 30
   days.
   Camphor vapours are toxic to adults only, larvae remain unaffected.
   In a dry atmosphere (0 \% R. H.) and at a constant 20-degrees-C,
   transpiration of adults and larvae is increased by paradichlorobenzene
   but it is not modified by camphor.
   In adults, heat resistance decreases in the presence of toxic vapours
   and results in a lowering of the thermostupor point (TSP). In adults,
   the TSP is 49.0+/-0.4-degrees-C (control), 39.8+/-5.4-degrees-C with
   paradichlorobenzene and 48.2+/-0.3-degrees-C with camphor. In larvae,
   the TSP is not modified by toxic vapours: 49.8+/-0.4-degrees-C for
   control, 48.5+/-1.0-degrees-C with paradichlorobenzene and
   50.0+/-1.3-degrees-C with camphor. Results suggest that the thermostupor
   point might be used in ecotoxicology to test the efficacy of a substance
   suspected to be insecticidal. The study also indicates that in
   holometabolous insects it is necessary to test every suspected
   insecticidal substance on all stages of development. In T bisselliella,
   adults are clearly more sensitive than larvae.}},
