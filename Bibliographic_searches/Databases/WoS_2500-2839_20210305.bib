@article{ ISI:A1997XU66700006,
Author = {Ali, MF},
Title = {{Development and heat tolerance of Anthrenus coloratus (Reitter)
   (Coleoptera: Dermestidae) reared on a laboratory diet}},
Journal = {{JOURNAL OF STORED PRODUCTS RESEARCH}},
Year = {{1997}},
Volume = {{33}},
Number = {{3}},
Pages = {{251-255}},
Month = {{JUL}},
Abstract = {{The number of larval instars of Anthrenus coloratus reared on dried baby
   milk plus pure wool textile at 33-35 degrees C, 70-80\% r.h., ranged
   from seven to nine. The mean duration of each larval instar from the
   first to the sixth was similar and shorter than those of later ones, The
   incubation period of eggs was 8.3 +/- 0.1 days, Larvae took a mean of
   101.7 +/- 4.4 days to complete development and pupate, Only 5.4 +/- 0.1
   days was recorded for the pupal period. Young and old larvae withstood
   starvation for 40.0 +/- 5.0 and 48.3 +/- 6.6 days, respectively, and
   19\% of the old larvae transformed to pupae, Starvation significantly
   affected the longevity of both mated and non-mated adult females but not
   of adult males, Only mated females were able to oviposit, and starvation
   had a considerable effect on fecundity as well as on the oviposition and
   post-oviposition periods, Larvae could tolerate high temperature
   exposure at 50 degrees C for only 4 min; mortality started after 6 min
   and increased to 100\% after 12 min. (C) 1997 Elsevier Science Ltd.}},
