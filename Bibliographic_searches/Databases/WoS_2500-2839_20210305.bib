@article{ ISI:A1994PL21500016,
Author = {HECHT, T},
Title = {{BEHAVIORAL THERMOREGULATION OF THE ABALONE, HALIOTIS-MIDAE, AND THE
   IMPLICATIONS FOR INTENSIVE CULTURE}},
Journal = {{AQUACULTURE}},
Year = {{1994}},
Volume = {{126}},
Number = {{1-2}},
Pages = {{171-181}},
Month = {{SEP 15}},
Abstract = {{Experiments were undertaken on two groups of South African abalone,
   Haliotis midae, with different thermal histories to determine their
   critical thermal maximum and their preferred temperature. There was no
   difference between the two groups in terms of their thermal preference
   and critical thermal maxima. The final temperature preferendum was
   determined in a horizontal thermal gradient chamber and was found to be
   24.1-24.5 degrees C, which was significantly higher than ambient
   environmental temperatures. The 50\% critical thermal maximum was 27.9
   degrees C. The results are discussed in relation to the natural
   distribution of the species, and the implications of these findings are
   considered for site selection and commercial rearing.}},
