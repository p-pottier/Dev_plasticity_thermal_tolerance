@article{ ISI:000072802800020,
Author = {Matsuda, H and Yamakawa, T},
Title = {{Effects of temperature on growth of the Japanese spiny lobster,
   Panulirus japonicus (V. Siebold) phyllosomas under laboratory conditions}},
Journal = {{MARINE AND FRESHWATER RESEARCH}},
Year = {{1997}},
Volume = {{48}},
Number = {{8, SI}},
Pages = {{791-796}},
Note = {{5th International Conference and Workshop on Lobster Biology and
   Management, QUEENSTOWN, NEW ZEALAND, FEB 10-14, 1997}},
Organization = {{Natl Inst Water \& Atmospher Res Ltd; New Zealand Fishing Ind Board; New
   Zealand Rock Lobster Ind Council}},
Abstract = {{The effects of temperature on growth of phyllosomas of the Japanese
   spiny lobster Panulirus japonicus were investigated Phyllosomas were
   individually reared at four temperatures (20 degrees, 22 degrees, 24
   degrees and 26 degrees C), and intermoult period and moult increment
   were monitored. The improved Belehradek's equation, G =
   aL(b)(T-alpha)(c)(beta-T)(d), was used to describe the relationship
   between growth characters (G: intermoult period, moult increment or
   daily growth increment), body length (L) and temperature (T) with a and
   b being constants that change at 17.8 mm body length, c and d being
   common constants for all sizes, and alpha and beta being the conceptual
   biological lower and upper critical temperatures, which are variables
   with body length. The intermoult period increased and the moult
   increment decreased with decreasing temperature. However, a negative
   influence on both the intermoult period and the moult increment was
   observed at 26 degrees C for medium-sized and large phyllosomas. The
   largest daily growth increment was obtained at 26 degrees C up to 15.0
   mm body length, and then at 24 degrees C. This indicates that the
   optimum temperature for growth decreases from 26 degrees to 24 degrees C
   at 15.0 mm body length. This optimum temperature for growth seemed to be
   optimal for survival.}},
