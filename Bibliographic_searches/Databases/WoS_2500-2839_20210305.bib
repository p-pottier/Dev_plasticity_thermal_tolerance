@article{ ISI:A1994QU93300011,
Author = {LEATHER, SR},
Title = {{THE EFFECT OF TEMPERATURE ON OVIPOSITION, FECUNDITY AND EGG HATCH IN THE
   PINE BEAUTY MOTH, PANOLIS-FLAMMEA (LEPIDOPTERA, NOCTUIDAE)}},
Journal = {{BULLETIN OF ENTOMOLOGICAL RESEARCH}},
Year = {{1994}},
Volume = {{84}},
Number = {{4}},
Pages = {{515-520}},
Month = {{DEC}},
Abstract = {{Over the temperature range 5 degrees to 25 degrees C, fecundity of
   Panolis flammea (Denis and Schiffermiiler) showed a non-linear response,
   the optimal temperature for fecundity being 15 degrees C. At 6 degrees C
   females laid 17 eggs each whilst at 15 degrees C they laid 166 eggs.
   Pre-oviposition period showed a significantly negative response, being
   shortest at 25 degrees C (2.5 days). Adult longevity was significantly
   affected by temperature, being 20 days at 8 degrees C and 6.8 days at 25
   degrees C. Egg hatch and development in the laboratory was linearly and
   positively related to temperature over the range of 2 degrees to 20
   degrees C, ranging from 39.9 days at 5 degrees C to 6 days at 25 degrees
   C. The lethal limit for egg viability was 25 degrees C. The theoretical
   threshold for egg development was calculated to be 2.79 degrees C.
   Storage of eggs at temperatures of 2 degrees C for periods in excess of
   10 days had a significant effect on the percentage of eggs hatching. The
   significance of these findings in implementing control operations is
   discussed.}},
