@article{ ISI:A1991FA14300010,
Author = {MENE, L and ALVAREZOSSORIO, MT and GONZALEZGURRIARAN, E and VALDES, L},
Title = {{EFFECTS OF TEMPERATURE AND SALINITY ON LARVAL DEVELOPMENT OF
   NECORA-PUBER (BRACHYURA, PORTUNIDAE)}},
Journal = {{MARINE BIOLOGY}},
Year = {{1991}},
Volume = {{108}},
Number = {{1}},
Pages = {{73-81}},
Abstract = {{Temperature and salinity affected both length of larval development and
   mortality in Necora puber collected in the Ria de A Coruna during
   December 1984 and January 1985. Development time decreased considerably
   with increased temperature. This decrease was sharper when temperature
   increased from 15-degrees to 20-degrees-C than when it increased from
   20-degrees to 25-degrees-C. At 35 parts per thousand S, average
   development took 48, 32 and 28 d at 15-degrees, 20-degrees and
   25-degrees-C, respectively. At the three salinities tested (25, 30 and
   35 parts per thousand), larval development was completed only at
   15-degrees-C, at 20-degrees-C/30 and 35 parts per thousand S, and at
   25-degrees-C/35 parts per thousand S. Development times at 15-degrees
   and 20-degrees-C were highly significantly different at both 35 and 30
   parts per thousand S (P << 0.01). However, there was no significant
   differences between development times at 20-degrees and 25-degrees-C (P
   > 0.05). Within any one specific temperature series, no significant
   difference was observed between the salinity values tested (P > 0.05).
   The duration of each of the five zoeal stages was similar within each
   and the same temperature/salinity combination, whereas the duration of
   the megalop was twice as long as any of the zoeal stages. The
   combination of the lowest temperature (15-degrees-C) and the highest
   salinity (35 parts per thousand) tested resulted in the greatest larval
   survival of 28\%. Highest mortality occurred at 25-degrees-C, at which
   temperature development was completed only at 35 parts per thousand S. A
   sharp drop in larval survival was observed in the transition period Zoea
   V - megalop in all combinations of temperature and salinity tested.
   Within the limits of tolerance to temperature and salinity, the former
   effected more pronounced differences in the duration of larval
   development, while salinity appeared to constitute a limiting factor for
   survival.}},
