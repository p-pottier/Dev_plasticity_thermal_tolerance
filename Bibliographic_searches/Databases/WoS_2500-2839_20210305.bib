@article{ ISI:A1993MK15700008,
Author = {SAWICKI, JE and CASAS, B},
Title = {{WET OXIDATION SYSTEM - PROCESS CONCEPT TO DESIGN}},
Journal = {{ENVIRONMENTAL PROGRESS}},
Year = {{1993}},
Volume = {{12}},
Number = {{4}},
Pages = {{275-283}},
Month = {{NOV}},
Abstract = {{A wet oxidation process was developed to treat waste water from an
   organic nitration process. The waste water contained EPA listed priority
   pollutants including dinitrotoluene, 2-nitrophenol, 4-nitrophenol,
   2,4-dinitrophenol, 4,6-dinitro-ortho-cresol and phenol. The process
   design was based on experimental data generated in both batch and
   continuous laboratory and pilot scale reactors and in cooperation with
   university researchers, a private development company and an engineering
   company. Process variables included temperatures between 250 and 500 C,
   pressures in the 23,000-28,000 kPa (3,450-4,000 psi) range and residence
   times between 1 and 7 minutes.
   The final design was a tubular reactor operating at temperatures
   slightly below the critical temperature for water and short residence
   times. Destruction efficiencies for most of the priority pollutants were
   greater than 99\% and final concentrations were below those specified by
   EPA regulations.}},
