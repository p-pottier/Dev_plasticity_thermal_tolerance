@article{ ISI:A1996UV39600008,
Author = {Baker, SC and Heidinger, RC},
Title = {{Upper lethal temperature tolerance of fingerling black crappie}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
Year = {{1996}},
Volume = {{48}},
Number = {{6}},
Pages = {{1123-1129}},
Month = {{JUN}},
Abstract = {{Upper lethal temperature tolerance was determined for 26-81 mm, age 0
   black crappie Pomoxis nigromaculatus of three size classes using both a
   rapid transfer and slow heating protocol. Rapid transfer protocols
   determined 24 LT(50) values of 33.8, 35.1 and 31.5 degrees C for size
   classes with mean total lengths of 30.2, 45.6 and 74.9 mm. A predictive
   model was generated from the slow heating protocols that relates lethal
   temperature to acclimation temperature, total length, and condition
   factor (K) as predictors. (C) 1996 The Fisheries Society of the British
   Isles}},
