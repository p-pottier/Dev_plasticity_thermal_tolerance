@article{ ISI:A1991EY52000002,
Author = {CHEN, CP and LEE, RE and DENLINGER, DL},
Title = {{COLD SHOCK AND HEAT-SHOCK - A COMPARISON OF THE PROTECTION GENERATED BY
   BRIEF PRETREATMENT AT LESS SEVERE TEMPERATURES}},
Journal = {{PHYSIOLOGICAL ENTOMOLOGY}},
Year = {{1991}},
Volume = {{16}},
Number = {{1}},
Pages = {{19-26}},
Month = {{MAR}},
Abstract = {{Brief exposure to low (0-degrees-C) or high (40-degrees-C) temperature
   elicits a protective response that prevents injury when the flesh fly,
   Sarcophaga crassipalpis Macquart, is subjected to more severe cold
   (-10-degrees-C) or heat (45-degrees-C).  Both the low and high
   temperature responses were found in all developmental stages of the fly,
   but were most pronounced in the pupal and pharate adult stages.  The
   protective responses generated by brief exposure to 0 or 40-degrees-C
   appear similar in that both result in a rapid acquisition of cold or
   heat tolerance and a loss of protection after the flies are returned to
   25-degrees-C.  The protection generated by chilling is obvious within 10
   min of exposure to 0-degrees-C while a 30 min exposure to 40-degrees-C
   is required to induce the high temperature protection.  High temperature
   protects against cold shock injury within a narrow range (around
   36-degrees-C) but we have no evidence that low temperature can protect
   against heat injury.  We previously demonstrated that the rapid increase
   in cold tolerance correlates with concomitant increases in glycerol
   concentration, but in this study we found no significant elevation in
   glycerol in heat-shocked flies.  Thus the physiological and biochemical
   bases for the rapid responses to cold and heat appear to be different.}},
