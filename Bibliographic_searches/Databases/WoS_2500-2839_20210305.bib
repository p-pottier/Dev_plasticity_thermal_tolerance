@article{ ISI:A1993LU71400014,
Author = {WU, DJ and HOU, RF},
Title = {{RELATIONSHIP BETWEEN THERMOTOLERANCE AND HEAT-STABLE ESTERASE IN THE
   SILKWORM, BOMBYX-MORI (LEPIDOPTERA, BOMBYCIDAE)}},
Journal = {{APPLIED ENTOMOLOGY AND ZOOLOGY}},
Year = {{1993}},
Volume = {{28}},
Number = {{3}},
Pages = {{371-377}},
Month = {{AUG}},
Abstract = {{Thermotolerance of the silkworm, Bombyx mori L., varied with strains
   when reared at 28C-degrees and 34-degrees-C, 70-85\% RH for 12 h. The
   thermotolerance was positively relative to the activity of a heat-stable
   esterase (Hs EST) analyzed in larval body or midgut. This enzyme
   activity could be adopted as an indicator for the silkworm
   thermotolerance. Zymograms showed that an E5 band in midgut preparations
   is predominant and is thus related to thermotolerance. The activity of
   Hs EST, especially E5, increased with heat selection for 7 generations,
   indicating that this enzyme seems to be induced by rearing the silkworms
   under high temperatures.}},
