@article{ ISI:A1993MC69900002,
Author = {TURNOCK, WJ},
Title = {{COLD-HARDINESS OF LACANOBIA-ATLANTICA (LEPIDOPTERA, NOCTUIDAE), AND A
   COMPARISON WITH 3 OTHER INSECT SPECIES THAT OVERWINTER IN THE SAME
   HABITAT}},
Journal = {{CANADIAN JOURNAL OF ZOOLOGY-REVUE CANADIENNE DE ZOOLOGIE}},
Year = {{1993}},
Volume = {{71}},
Number = {{9}},
Pages = {{1710-1714}},
Month = {{SEP}},
Abstract = {{The pupae of Lacanobia atlantica (Grt.) overwinter in the soil and are
   susceptible to freezing. Cold injury and death occur at temperatures
   above the supercooling point (SCP) of -26.4-degrees-C, and below ca.
   -12.6-degrees-C. Mortality increases as temperature decreases and as
   exposure lengthens. These results are compared with those for Mamestra
   configurata Wlk., Delia radicum (L.), and Athrycia cinerea Coq., which
   have similar overwintering habits. Their mean SCPs (-27.3 to
   -20.3-degrees-C) differ less than do the values representing the highest
   temperature at which cold injury occurs (almost-equal-to -12.6 to
   almost-equal-to 4-degrees-C). Response curves of three of the species
   were similar for changes in temperature and duration of exposure, and
   similarly in a plot of the time to 50\% survival (LT50) against
   temperature. They differed only in their relative cold-hardiness. In
   contrast, A. cinerea was less sensitive to changes in temperature and
   duration of exposure. A single parameter (e.g., the mean SCP) is not
   adequate to describe the cold-hardiness of these species because they
   vary not only in the upper and lower bounds of their cold-injury zones
   but also in their response to temperature and duration of exposure
   within this zone.}},
