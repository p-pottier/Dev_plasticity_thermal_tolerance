@article{ ISI:000072101800006,
Author = {Schirocki, AG and Hague, NGM},
Title = {{The effect of selective culture of Steinernema feltiae at low
   temperature on establishment, pathogenicity, reproduction and size of
   infective juveniles}},
Journal = {{NEMATOLOGICA}},
Year = {{1997}},
Volume = {{43}},
Number = {{6}},
Pages = {{481-489}},
Month = {{NOV}},
Abstract = {{Steinernema feltiae was selectively cultured in Galleria mellonella as
   follows: 13 infective cycles at 22 degrees C, near to the optimum
   temperature for development of S. feltiae, four infective cycles at 10
   degrees C close to the lowest temperature at which reproduction occurs,
   or four infective cycles at 10 degrees C followed by one cycle at 22
   degrees C. After four cycles at 10 degrees C the LT50 (Galleria
   mortality) was shorter, the establishment higher and the size of
   infective juveniles longer and wider than those produced after 13
   infective cycles at 22 degrees C. After treatment at four cycles at 10
   degrees C followed by one cycle at 22 degrees C the decrease in LT50
   observed after four cycles at 10 degrees C was lost when tested at 7
   degrees C and 10 degrees C and partially lost when tested at 12 degrees
   C and 15 degrees C. Reproduction was not affected by previous culture
   temperatures.}},
