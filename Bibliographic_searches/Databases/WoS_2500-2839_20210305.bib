@article{ ISI:A1992JE17200004,
Author = {JAMES, NPE and BRUTON, MN},
Title = {{ALTERNATIVE LIFE-HISTORY TRAITS ASSOCIATED WITH REPRODUCTION IN
   OREOCHROMIS-MOSSAMBICUS (PISCES, CICHLIDAE) IN SMALL WATER BODIES OF THE
   EASTERN CAPE, SOUTH-AFRICA}},
Journal = {{ENVIRONMENTAL BIOLOGY OF FISHES}},
Year = {{1992}},
Volume = {{34}},
Number = {{4}},
Pages = {{379-392}},
Month = {{AUG}},
Abstract = {{Four wild populations of Oreochromis mossambicus were investigated to
   examine the effects of environmental factors on reproductive biology in
   contrasting habitats. Age at maturity, spawning-season length, spawning
   frequency, clutch size and nesting behaviour were influenced by the
   prevailing conditions for feeding, breeding and refuge. Even under a
   temperature regime that approached lethal limits during winter, and
   erratically fluctuating water levels, O. mossambicus demonstrated an
   ability to adopt a precocial life-history style in some habitats. In
   more `hostile' water bodies an increasingly altricial style was
   followed.}},
