@article{ ISI:A1991GG91200004,
Author = {SHIMADA, K and OHYAMA, Y and PAN, CX},
Title = {{COLD-HARDINESS OF THE ANTARCTIC WINGED MIDGE PAROCHLUS-STEINENII DURING
   THE ACTIVE SEASON AT KING-GEORGE ISLAND}},
Journal = {{POLAR BIOLOGY}},
Year = {{1991}},
Volume = {{11}},
Number = {{5}},
Pages = {{311-314}},
Month = {{SEP}},
Abstract = {{The Antarctic winged midge Parochlus steinenii (Diptera:  Chironomidae)
   and its immatures were collected from freshwater lakes near Great Wall
   Station (62-degrees 13'S,58-degrees 58'W) on King George Island during
   January and February in 1990.  They were examined for supercooling
   ability and lower lethal temperature.  They were all intolerant to
   freezing.  Supercooling points (spontaneous freezing points) of the
   larvae, pupae and adults were -7.4-degrees +/- 1.0-degrees,
   -16.3-degrees +/- 4.6-degrees and -15.3-degrees +/- 5.6-degrees-C,
   respectively.  These values represented the potential limits of
   cold-hardiness of this species.  But the median lower lethal
   temperatures examined under an aqueous condition were always higher than
   the corresponding mean supercooling points.  Half of the larvae, pupae
