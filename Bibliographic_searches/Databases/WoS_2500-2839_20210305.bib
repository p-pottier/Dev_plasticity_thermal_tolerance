@inproceedings{ ISI:000073875400048,
Author = {Foliaki, S and Armstrong, JW},
Editor = {{Allwood, AJ and Drew, RAI}},
Title = {{Heat tolerances of immature stages of Bactrocera facialis and
   B-xanthodes (Diptera : Tephritidae)}},
Booktitle = {{MANAGEMENT OF FRUIT FLIES IN THE PACIFIC}},
Series = {{ACIAR PROCEEDINGS SERIES}},
Year = {{1997}},
Number = {{76}},
Pages = {{239-246}},
Note = {{Regional Symposium on the Management of Fruit Flies in the Pacific,
   NADI, FIJI, OCT 28-31, 1996}},
Organization = {{Austr Agcy Int Dev; Austr Ctr Int Agr Res; UN Dev Programme; New Zealand
   Govt; European Union Plant Protect Serv, S Pacific Commiss; UN FAO;
   Crawford Fund Int Agr Res}},
Abstract = {{The effects of time and temperature on the egg and larval stages of two
   fruit fly species, Bactrocera facialis (Coquillett) and B. xanthodes
   (Broun), were studied using hot water immersions at temperatures of 44
   degrees C to 49 degrees C and for 0 to 30 min depending on the
   treatment, lifestage, and immersion temperature. Separate tests (169
   total) were done with eggs (all ages, early-aged, or late-aged), first
   instars, and third instars (feeding and non-feeding).
   B. facialis were significantly more heat tolerant than B. xanthodes for
   all life stages at all temperatures. For temperatures <46 degrees C,
   first instars were the most tolerant (LT99 = 21 min at 45 degrees C). At
   46 degrees C, late-aged eggs were the most tolerant (LT99 = 12 minutes
   at 46 degrees C). At temperatures >46 degrees C, third instar were the
   most tolerant and the non-feeding third instars were more tolerant (LT99
   = 6.4 min at 47 degrees C) than the feeding third instars. The late-aged
   eggs were the most tolerant of all egg ages, and there was some evidence
   that above 46 degrees C, they were less tolerant than any of the larval
   instars.}},
