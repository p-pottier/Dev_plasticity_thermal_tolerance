@article{ ISI:A1995QG57100009,
Author = {DONAHAYE, EJ and NAVARRO, S and RINDNER, M},
Title = {{LOW-TEMPERATURE AS AN ALTERNATIVE TO FUMIGATION FOR DISINFECTING DRIED
   FRUIT FROM 3 INSECT SPECIES}},
Journal = {{JOURNAL OF STORED PRODUCTS RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{1}},
Pages = {{63-70}},
Month = {{JAN}},
Abstract = {{The effect of exposure to 0, -5, -10 and -18-degrees-C was investigated
   on all developmental stages of the stored product insects Tribolium
   castaneum (Herbst), Oryzaephilus surinamenisis (L.), and Ephestia
   cautella (Walker).  For all three species the estimated time to produce
   99\% kill of the least sensitive stage of the least sensitive species
   ranged from 625 h for pupae of E. cautella at 0-degree-C, 55 h for pupae
   of E. cautella at - 5-degrees-C, 6.1 for egges of E. cautella at -
   10-degrees-C, to 2.6 h for eggs of E. cautella at - 18-degrees-C. 
   Interspecific comparisons of mortalities were made at the LT50 level,
   due to the wide confidence limits at high mortalities especially at the
   lower temperatures.  For T. castaneum the adult was least sensitive at 0
   and -5-degrees-C and the pupa at - 10 and -18-degrees-C.  For E.
   cautella the least sensitive stage at 0 and -5-degrees-C was the pupa,
   and at -10 and -18-degrees-C the egg.  For O. surinamensis the least
   sensitive stage at 0-degree-C was the larva, at -5 and -18-degrees-C the
   adult, and at -10-degrees-C the pupa.}},
