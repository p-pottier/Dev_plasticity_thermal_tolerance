@article{ ISI:A1994MU97200009,
Author = {PULLIN, AS},
Title = {{COLD TOLERANCE OF AN INSECT PARASITOID COTESIA (APANTELES) GLOMERATUS
   AND A COMPARISON WITH THAT OF ITS HOST PIERIS-BRASSICAE AND A
   HYPERPARASITOID TETRASTICHUS-GALACTOPUS}},
Journal = {{CRYO-LETTERS}},
Year = {{1994}},
Volume = {{15}},
Number = {{1}},
Pages = {{67-74}},
Month = {{JAN-FEB}},
Abstract = {{The cold tolerance strategy of diapausing larvae of Cotesia glomeratus
   was assessed by; 1. measurement of supercooling points, their variation
   overwinter and in response to low temperature acclimation; 2. by
   survival at non-freezing subzero temperatures (-10 and -20-degrees-C)
   and; 3. by measurement of the overwintering carbohydrate profiles. Mean
   supercooling points did not change significantly overwinter or after
   acclimation, remaining between -25 and -26-degrees-C. Exposure to
   -20-degrees-C resulted in an LT50 of <1 day in October rising to 2.2
   days in March. Similarly, exposure to -10-degrees-C resulted in an LT50 
   of 5.9 days in October rising to 19.8 days in December and >28 days in
   March suggesting an increase in cold tolerance over this period. A
   hyperparasitoid, Tetrastichus galactopus survived up to 28 days at
   -20-degrees-C suggesting a significantly greater non-freezing cold
   tolerance. Glycerol is the most abundant carbohydrate in C. glomeratus
   during winter, accumulating to a maximum of 40.5 mg/g wet weight (0.63
   molar) during March. Mean trehalose concentrations fluctuated between 11
   and 19 mg/g wet weight from October until March after which it decreased
   to 1.9 mg/g in May. The importance of the comparative cold tolerance of
   A. glomeratus and its common host Pieris brassicae in predicting
   population dynamics is discussed,}},
