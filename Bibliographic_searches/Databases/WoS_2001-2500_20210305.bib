@article{ ISI:000166510400007,
Author = {Subramanian, VT},
Title = {{Burrowing habits in juveniles of marine prawn Metapenaeus dobsoni
   (Crustacea : Decapoda)}},
Journal = {{INDIAN JOURNAL OF MARINE SCIENCES}},
Year = {{2000}},
Volume = {{29}},
Number = {{4}},
Pages = {{329-332}},
Month = {{DEC}},
Abstract = {{Juveniles of M.dobsoni revealed an intrinsic circadian rhythm, burrowing
   to rest quietly in day and moving actively at night, and the schedule of
   changing over the phase is sunrise and sunset. The intensity of light is
   the prime factor stimulating burrowing habit that the juveniles would
   burrow on subjected to light in night and emerge to move actively under
   reduced light in day. Several factors, such as, cloudiness, temperature,
   salinity and availability of food interrupt burrowing. The critical
   temperature limits are 18-32 degreesC and below 10 degreesC is fatal.
   The juveniles measuring <25 mm size have no response to light and do not
   burrow as larger ones.}},
