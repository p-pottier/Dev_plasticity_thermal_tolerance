@article{ ISI:000171790100004,
Author = {Atwood, HL and Young, SP and Tomasso, JR and Smith, TIJ},
Title = {{Salinity and temperature tolerances of black sea bass juveniles}},
Journal = {{NORTH AMERICAN JOURNAL OF AQUACULTURE}},
Year = {{2001}},
Volume = {{63}},
Number = {{4}},
Pages = {{285-288}},
Month = {{OCT}},
Abstract = {{The salinity and temperature tolerances of black sea bass Centropristis
   striata juveniles (46 +/- 10.3 mm total length {[}mean +/- SD]) were
   estimated to assist in decisions concerning location of culture
   facilities and types of production systems. At 21 degreesC, all exposed
   fish survived for 7 d in salt concentrations of 10, 20, or 35 g/L. All
   fish exposed to salt concentrations of 5 g/L died within 3 d.
   Temperature extremes were estimated after acclimation of fish to 24-25
   degreesC. Half of the juveniles exposed to a gradual increase (+0.65
   degreesC/d) or decrease (-0.51 degreesC/d) in temperature (at a nominal
   salt concentration of 20 g/L) were dead when the water temperature
   reached 33.3 degreesC and 3.7 degreesC, respectively. Results of this
   study indicate that this species requires saline water at or above a
   salt concentration of 10 g/L and that it may not be practical to keep
   black sea bass over winter or over summer in shore-based outdoor
   facilities located in temperate climates.}},
