@article{ ISI:000231160400005,
Author = {Das, T and Pal, AK and Chakraborty, SK and Manush, SM and Sahu, NP and
   Mukherjee, SC},
Title = {{Thermal tolerance, growth and oxygen consumption of Labeo rohita fry
   (Hamilton, 1822) acclimated to four temperatures}},
Journal = {{JOURNAL OF THERMAL BIOLOGY}},
Year = {{2005}},
Volume = {{30}},
Number = {{5}},
Pages = {{378-383}},
Month = {{JUL}},
Abstract = {{(A) 30 day feeding trial was conducted using a freshwater fish, Labeo
   rohita (rohu), to determine their thermal tolerance, oxygen consumption
   and optimum temperature for growth. Four hundred and sixteen L. rohita
   fry (10 days old, 0.385 +/- 0.003 g) were equally distributed between
   four treatments (26, 31, 33 and 36 degrees C) each with four replicates
   for 30 days. Highest body weight gain and lowest feed conversion ratio
   (FCR) was recorded between 31 and 33 degrees C. The highest specific
   growth rate was recorded at 31 degrees C followed by 33 and 26 degrees C
   and the lowest was at 36 degrees C. Thermal tolerance and oxygen
   consumption studies were carried out after completion of growth study to
   determine tolerance level and metabolic activity at four different
   acclimation temperatures. Oxygen consumption rate increased
   significantly with increasing acclimation temperature. Preferred
   temperature decided from relationship between acclimation temperature
   and Q(10) values were between 3 3 and 36 degrees C, which gives a better
   understanding of optimum temperature for growth of L. rohita. Critical
   thermal maxima(CTMax) and critical thermal minima (CTMin) were 42.33 +/-
   0.07, 44.81 +/- 0.07, 45.35 +/- 0.06, 45.60 +/- 0.03 and 12.00 +/- 0.08,
   12.46 +/- 0.04, 13.80 +/- 0.10, 14.43 +/- 0.06, respectively, and
   increased significantly with increasing acclimation temperatures (26,
   31, 33 and 36 degrees C). Survival (\%) was similar in all groups
   indicating that temperature range of26-36 degrees C is not fatal to L.
   rohita fry. The optimum temperature range for growth was 31-33 degrees C
   and for Q(10) values was 33-36 degrees C. (c) 2005 Elsevier Ltd. All
   rights reserved.}},
