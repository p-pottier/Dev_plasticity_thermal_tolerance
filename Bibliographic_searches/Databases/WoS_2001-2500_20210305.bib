@article{ ISI:000073139800039,
Author = {Whiting, DC and Hoy, LE},
Title = {{Effect of temperature establishment time on the mortality of Epiphyas
   postvittana (Lepidoptera : Tortricidae) larvae exposed to a
   high-temperature controlled atmosphere}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{1998}},
Volume = {{91}},
Number = {{1}},
Pages = {{287-292}},
Month = {{FEB}},
Abstract = {{Mortality responses of 1st-, 3rd-, and 5th-instar Epiphyas postivittana
   (Walker) on apples to a controlled atmosphere (1\% O-2, 1\% CO2) with 4
   different times (5-12 h) to increase fruit surface temperature from 20
   degrees C (temperature during controlled atmosphere establishment) to 40
   degrees C (target temperature) were determined. Percentage mortality of
   all larval stages tested increased linearly on a complementary log-log
   scale with increase in treatment time. E. postvittana showed a general
   pattern of life-stage tolerance to treatment of Ist instar less than or
   equal to 3rd instar less than or equal to 5th instar. For all larval
   stages, mean lethal time (LT) estimates increased with increasing
   temperature establishment time. However, almost all insect mortality
   occurred within the period of temperature establishment. LT(50)s for all
   larval stages and LT(99)s for Ist instars increased less than the
   increase in temperature establishment time. Increasing the temperature
   establishment time increased the LT99 estimates of 3rd and 5th instars
   by an equivalent amount. No mortality estimate of any larval stage
   increased more than the increase in temperature establishment time.}},
