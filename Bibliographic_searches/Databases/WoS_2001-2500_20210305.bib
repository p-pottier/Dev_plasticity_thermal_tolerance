@article{ ISI:000182166400037,
Author = {Johnson, JA and Wang, S and Tang, J},
Title = {{Thermal death kinetics of fifth-instar Plodia interpunctella
   (Lepidoptera : Pyralidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
