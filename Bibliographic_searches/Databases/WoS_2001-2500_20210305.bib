@article{ ISI:000189114200005,
Author = {Fisher, JR and Edwards, DL},
Title = {{Temperature-dependent egg hatch and cold storage of eggs of Otiorhynchus
   ovatus (L.) (Coleoptera : Curculionidae)}},
Journal = {{JOURNAL OF AGRICULTURAL AND URBAN ENTOMOLOGY}},
Year = {{2002}},
Volume = {{19}},
Number = {{2}},
Pages = {{109-116}},
Month = {{APR}},
Abstract = {{Egg hatch of the strawberry root weevil, Otiorhynchus ovatus (L.) was
   studied at constant temperature intervals from 6 to 36degreesC. Storage
   of eggs for use in rearing and pot and field infestations was studied at
   six low-temperature treatments (<16\&DEG;C) for storage intervals up to
   15 wk. Limited hatch was obtained at 6 and 33\&DEG;C but mortality was
   >85\%. Preliminary studies had shown that there was 100\% mortality at 3
   and 36degreesC. We conclude that the lower threshold for egg development
   would be similar to4degreesC and the upper lethal temperature was
   between 33 and 36degreesC. There was <10\% mortality at all other
   temperatures (>6degreesC <33\&DEG;C). Rate of development of the eggs to
   hatch was fit to a nonlinear curve. The lowest mortality and least
   amount of time for duration of hatch were found at 21\&DEG;C. After
   examining mortality, rate of development, and the overall duration of
   hatch, we concluded that the optimum temperature range for O. ovatus egg
   hatch was 18-27\&DEG;C. We found that eggs could be stored and be >60\%
   viable at a temperature of 4degreesC for up to 4 wk.}},
