@article{ ISI:000180786900014,
Author = {Bull, EL and Shepherd, JF},
Title = {{Water temperature at oviposition sites of Rana luteiventris in
   northeastern Oregon}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
Year = {{2003}},
Volume = {{63}},
Number = {{1}},
Pages = {{108-113}},
Month = {{JAN}},
Abstract = {{Water temperature at which Columbia spotted frog (Rana luteiventris)
   eggs were deposited and developed was determined at 18 oviposition sites
   in northeastern Oregon in 2000. Mean daily water temperature on the day
   of initial egg deposition ranged from 7.6degreesC to 16.0degreesC with a
   mean of 9.6degreesC. Maximum water temperatures on the 1st day of egg
   deposition ranged from 9.8degreesC to 20.2degreesC and averaged
   15.5degreesC. Eggs were not deposited on days when maximum water
   temperature was below 9.4degreesC. Duration of egg deposition ranged
   from 1 to 20 days at the 18 sites. Embryos hatched after 12-21 days.
   Mean and maximum daily water temperatures were significantly correlated
   with number of days to hatching.}},
