@inproceedings{ ISI:000177423200019,
Author = {Frenette, B and Parsons, GJ},
Editor = {{Hendry, CI and McGladdery, SE}},
Title = {{Salinity-temperature tolerance of juvenile giant scallops, Placopecten
   magellanicus}},
Booktitle = {{AQUACULTURE CANADA 2000}},
Series = {{Aquaculture Association of Canada-Special Publication}},
Year = {{2001}},
Number = {{4}},
Pages = {{76-78}},
Note = {{17th Annual Meeting of the Aquculture-Association-of-Canada, MONCTON,
   CANADA, MAY 28-31, 2000}},
Organization = {{Univ Moncton; Atantic Canada Opportunities Agcy; New Brunswick, Dept
   Agr, Fisheries Aquaculture}},
Abstract = {{When the giant (sea) scallop, Placopecten magellanicus, is cultured in
   shallow water embayments, salinity and water temperature are important
   environmental parameters influencing its survival and growth. This
   laboratory study evaluated the survival (24-240 hour LC50 bioassays) of
   P. magellanicus at different water temperatures (3, 8, 13, 18, and
   23degreesC) and salinities (5, 10, 15, 20, 25, and 30ppt). Two sizes
   classes (1020 mm and 20-35 mm) of juvenile scallops were examined. Small
   scallops had a similar survival rate to the large scallops at all water
   temperatures and salinities. Both size classes of juveniles were less
   tolerant to salinity at increasing temperatures and increasing time of
   exposure. The 96-h LC50's were 13, 13, 13, 14.3, and 22.3 ppt and 13,
   13, 13, 14.6, and 20.2 ppt for small and large scallops at 3 to
   23degreesC, respectively. At 23degreesC, mortality increased rapidly in
   small scallops after 144 hours and for the large scallops after 24
   hours. In order to have about 100\% survival after 10 days, the salinity
   should be >25ppt and temperature <18degreesC Knowledge of the salinity
   and temperature characteristics, and sources of freshwater are essential
   before establishing a scallop aquaculture site.}},
