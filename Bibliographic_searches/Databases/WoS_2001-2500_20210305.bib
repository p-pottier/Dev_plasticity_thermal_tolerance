@article{ ISI:000230047800012,
Author = {Sorensen, JG and Norry, FM and Scannapieco, AC and Loeschcke, V},
Title = {{Altitudinal variation for stress resistance traits and thermal
   adaptation in adult Drosophila buzzatii from the New World}},
Journal = {{JOURNAL OF EVOLUTIONARY BIOLOGY}},
