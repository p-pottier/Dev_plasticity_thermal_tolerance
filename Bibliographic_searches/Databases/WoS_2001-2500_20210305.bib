@article{ ISI:000181802700041,
Author = {Hamasaki, K},
Title = {{Effects of temperature on the egg incubation period, survival and
   developmental period of larvae of the mud crab Scylla serrata (Forskal)
   (Brachyura : Portunidae) reared in the laboratory}},
Journal = {{AQUACULTURE}},
Year = {{2003}},
Volume = {{219}},
Number = {{1-4}},
Pages = {{561-572}},
Month = {{APR 2}},
Abstract = {{This study examined effects of temperature on the egg incubation period,
   survival and developmental period of larvae of the mud crab Scylla
   serrata reared in the laboratory. Pre-mated females were held in tanks
   in which temperature varied seasonally; their spawning and hatching were
   recorded daily. The number of days from spawning to hatching and mean
   rearing temperature were determined for 92 females. The egg incubation
   period decreased exponentially from 30 to 10 days with increasing mean
   temperature in the range 20.3-30.0 degreesC. Larvae were reared in 1-l
   beakers at seven temperatures (17, 20, 23, 26, 29, 32 and 35 degreesC)
   and fed with rotifers and Artemia nauplii. Survival to the first crab
   stage occurred when larvae were reared at 23-32 degreesC: the best
   survival rate was obtained at 29 degreesC. Also, the number of days from
   hatching to attainment of each larval stage decreased with increasing
   temperature. Relationships between mean temperature (T) and egg
   incubation period and larval developmental period from hatching to reach
   the megalopal stage (D) were analyzed using the following three equation
   models: power function, D = aT(b); Belehradek's equation, D = a(T -
   alpha)(b); and from heat summation theory, D = a/(T - alpha). For both
   egg and larval development, the equation of heat summation theory was
   the best model to characterize the relationship between temperature and
   the number of days required for development. The biological lower
   critical temperature represented by parameter alpha was similar for eggs
   and larvae: 13.93 and 13.82 degreesC, respectively, for Belehradek's
   equation and 15.70 and 15.08 degreesC for heat summation theory. (C)
   2003 Elsevier Science B.V. All rights reserved.}},
