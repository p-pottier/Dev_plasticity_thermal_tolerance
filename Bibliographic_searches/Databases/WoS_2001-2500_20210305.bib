@article{ ISI:000169127000006,
Author = {Joshi, A and Tiwari, PK},
Title = {{Chromosomal responses of blowfly Lucilia cuprina to heat and heavy metal
   stress}},
Journal = {{GENETICA}},
Year = {{2000}},
Volume = {{109}},
Number = {{3}},
Pages = {{211-218}},
Abstract = {{Chromosomal responses to heat and heavy metal shocks were studied in the
   pupal trichogen polytene chromosomes of Lucilia cuprina. Heat shock
   induced seven distinct puffs on different chromosomes at the following
   loci: 1C2, 21A1, 21C, 23B, 24B, 42A and 95B3. Arsenate and mercury, two
   of the most common toxic environmental chemical pollutants also, induced
   almost the same set of puffs except 20B2, which appeared to be induced
   by metals only and 95B3, which was not induced by arsenate. The findings
   suggest that a common set of gene loci encoding the heat shock proteins
   is responsive to these diverse environmental stresses.}},
