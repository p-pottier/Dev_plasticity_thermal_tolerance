@article{ ISI:000089183600012,
Author = {Dickerson, BR and Vinyard, GL},
Title = {{Effects of high chronic temperatures and diel temperature cycles on the
   survival and growth of Lahontan cutthroat trout}},
Journal = {{TRANSACTIONS OF THE AMERICAN FISHERIES SOCIETY}},
Year = {{1999}},
Volume = {{128}},
Number = {{3}},
Pages = {{516-521}},
Month = {{MAY}},
Abstract = {{Tests of chronic temperature tolerance indicated that the upper limit
   for survival and growth of Lahontan cutthroat trout Oncorhynchus clarki
   henshawi was between 22 degrees C and 24 degrees C. Lahontan cutthroat
   trout experienced complete mortality within 2 d at 28 degrees C and
   approximately 60\% mortality over 7 d at 26 degrees C. Fish suffered no
   significant mortality at temperatures of 24 degrees C and below. No
   significant differences in growth of fish held at 22 degrees C relative
   to fish held at cooler temperatures were found, but significant
   reductions in growth were observed at 24 degrees C. When fish were
   subjected to fluctuating temperatures similar to field conditions, which
   cycle daily from 20 degrees C to 26 degrees C, they still grew, though
   not as much as fish maintained at constant 13 degrees C or 20 degrees C.
   Exposure to diel temperature fluctuations did not affect tolerance of
   subsequent chronic elevated temperatures.}},
