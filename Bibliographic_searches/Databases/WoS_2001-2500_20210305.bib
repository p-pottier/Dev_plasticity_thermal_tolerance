@article{ ISI:000237433100003,
Author = {Shimada, Yukinori and Shikano, Takahito and Seikai, Tadahisa},
Title = {{Ontogenetic changes in tolerance for environmental stress in Japanese
   flounder Paralichthys olivaceus during early development}},
Journal = {{NIPPON SUISAN GAKKAISHI}},
Year = {{2006}},
Volume = {{72}},
Number = {{2}},
Pages = {{169-173}},
Month = {{MAR}},
Abstract = {{Ontogenetic changes in physiological tolerance during larval development
   of Japanese flounder Paralichthys olivaceus were examined relative to a
   variety of environmental stresses as follows: freshwater, high salinity
   (salinity range: 30-55), high water temperature (temperature range:
   20-36 degrees C) and formalin (concentration range: 0-0.6\%). Highest
   stress tolerance was shown by newly hatched larvae (1 day after
   hatching), and then decreased drastically until 10 days after hatching.
   We considered that the high sensitivity to external environmental
   stresses at 10 days after hatching was caused by the development of the
   mouth and opercula. Patterns of tolerance change during the early life
   stages differed depending on the environmental stresses.}},
