@article{ ISI:000088598600018,
Author = {Appel, AG and Tanley, MJ},
Title = {{Laboratory and field performance of an imidacloprid gel bait against
   German cockroaches (Dictyoptera : Blattellidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{2000}},
Volume = {{93}},
Number = {{1}},
Pages = {{112-118}},
Month = {{FEB}},
Abstract = {{An experimental 2.15\% imidacloprid gel bait containing approximate to
   44\% water was evaluated in laboratory and field studies against the
   German cockroach, Blattella germanica (L.). In continuous exposure
   tests, toxicity and presumably bait consumption varied with cockroach
   stage, deprivation of competitive food, and temperature. The LT50 values
   for cockroaches provided with competitive food ranged from approximate
   to 0.9 h for adult females to 190 h for small nymphs. The LT(50)s for
   cockroaches not provided competitive food ranged from approximate to 1.7
   h for adult females to approximate to 31 h for adult males. The LT(50)s
   decreased exponentially with temperature between 10 and 30 degrees C.
   Even though the bait was significantly more repellent (approximate to
   38\%) than an untreated control (approximate to 14\%) when tested in
   Ebeling choice boxes, performance index values were positive and
   increased to nearly 100 (indicating high mortality and low repellency)
   after 14 d. When applied at 15-45 g per kitchen, the bait significantly
   reduced German cockroach trait catch in infested homes during a 4-wk
   period. There was a approximate to 50\% reduction after 1 wk and
   approximate to 80\% reduction 4 wk after treatment.}},
