@article{ ISI:000241730200015,
Author = {Cook, A. M. and Duston, J. and Bradford, R. G.},
Title = {{Thermal tolerance of a northern population of striped bass Morone
   saxatilis}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
Year = {{2006}},
Volume = {{69}},
Number = {{5}},
Pages = {{1482-1490}},
Month = {{NOV}},
Abstract = {{Thermal tolerance of age 0+ year Shubenacadie River (Nova Scotia,
   Canada) striped bass Morone saxatilis juveniles (mean +/- S.E. fork
   length, L-F, 19.2 +/- 0.2 cm) acclimated in fresh water to six
   temperatures from 5 to 30 degrees C was measured by both the incipient
   lethal technique (72 h assay), and the critical thermal method (C-m).
   The lower incipient lethal temperature ranged from 2.4 to 11.3 degrees
   C, and the upper incipient lethal temperature (I-U) from 24.4 to 33.9
   degrees C. The area of thermal tolerance was 618 degrees C-2. In a
   separate experiment, the I-U of large age 2+ year fish (34.4 +/- 0.5 cm
   L-F) was 1.2 and 0.6 degrees C lower (P < 0.01) than smaller age 1+ year
   fish (21.8 +/- 0.5 cm L-F) at acclimation temperatures of 16 and 23
   degrees C. Using the C-m, loss of equilibrium occurred at 27.4-37.7
   degrees C, loss of righting response at 28.1-38.4 degrees C and onset of
   spasms at 28.5-38.8 degrees C, depending on acclimation temperature. The
   linear regression slopes for these three responses were statistically
   similar (0.41; P > 0.05), but the intercepts differed (25.3, 26.0 and
   26.5 degrees C; P < 0.01). The thermal tolerance of this northern
   population appears to be broader than southern populations. (c) 2006 The
   Authors Journal compilation (c) 2006 The Fisheries Society of the
   British Isles.}},
