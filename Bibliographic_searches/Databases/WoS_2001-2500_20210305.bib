@article{ ISI:000085550100012,
Author = {Chung, KS},
Title = {{Heat resistance and thermal acclimation rate in tropical tetra Astyanax
   bimaculatus of Venezuela}},
Journal = {{ENVIRONMENTAL BIOLOGY OF FISHES}},
Year = {{2000}},
Volume = {{57}},
Number = {{4}},
Pages = {{459-463}},
Month = {{APR}},
Abstract = {{Venezuelan river tetra, Astyanax bimaculatus juveniles of 34.1-36.7 mm
   standard length and 0.83-1.0 g wet weight were acclimated for four weeks
   to 24-33 degrees C, which are approximate average minimum and maximum
   river temperatures throughout the year. The fish acclimated to 24, 27,
   30, and 33 degrees C were exposed for 10 000 minutes at 35, 36, 37, 38,
   and 39 degrees C to determine individual heat resistance times. To
   determine acclimation rates, the juveniles acclimated to 24 and 30
   degrees C were tested for individual heat resistance times at 39 degrees
   C by changing acclimation temperatures. The individual heat resistance
   times were increased in accordance with an increase in acclimation
   temperature and a decrease in test temperature, indicating that
   acclimation level has a great influence on thermal resistance of the
   fish tested. As the fish were transferred from 24 to 30 degrees C
   (upward acclimation), they completed their acclimation level in a few
   days, while those transferred from 30 to 24 degrees C (downward
   acclimation) required about 14 days. It has reaffirmed the following
   general behavior: the rate of gain in thermal resistance is fast and the
   loss in heat tolerance is very slow. This physiological phenomenon is
   very important for tropical fish, which acclimates rapidly in rising
   temperature during the hot day and does not lose this level in
   decreasing temperature during the cool night. Consequently, a tropical
   fish can maintain its maximum resistance level, adapt well in thermally
   fluctuating tropical waters, and survive in lethally high temperatures
   caused by a sudden increase in temperature during hot day.}},
