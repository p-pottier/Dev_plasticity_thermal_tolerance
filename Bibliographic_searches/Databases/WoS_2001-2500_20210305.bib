@article{ ISI:000205261500018,
Author = {Du, Wei-Guo and Zheng, Rong-Quan and Shu, Lin},
Title = {{The Influence of Incubation Temperature on Morphology, Locomotor
   Performance, and Cold Tolerance of Hatchling Chinese Three-Keeled Pond
   Turtles, Chinemys reevesii}},
Journal = {{CHELONIAN CONSERVATION AND BIOLOGY}},
Year = {{2006}},
Volume = {{5}},
Number = {{2}},
Pages = {{294-299}},
Month = {{DEC}},
Abstract = {{We incubated eggs of the Chinese three-keeled pond turtle (Chinemys
   reevesii) at 4 constant temperatures to assess the influence of these
   thermal regimes on incubation length, hatching success, and phenotypic
   traits of hatchlings. Eggs incubated at 24 degrees and 27 degrees C
   produced larger hatchlings in body mass, carapace size, and limb length
   than did those from 30 degrees and 33 degrees C. Hatchlings from 27
   degrees and 30 degrees C crawled and swam faster than did their
   counterparts from 24 degrees and 33 degrees C. The critical thermal
   minima of hatchlings incubated at 30 degrees and 33 degrees C were
   higher than those of hatchlings from 24 degrees and 27 degrees C.}},
