@article{ ISI:000184062300002,
Author = {Myrick, CA and Cech, JJ},
Title = {{The physiological performance of golden trout at water temperatures of
   10-19 degrees C}},
Journal = {{CALIFORNIA FISH AND GAME}},
Year = {{2003}},
Volume = {{89}},
Number = {{1}},
Pages = {{20-29}},
Month = {{WIN}},
Abstract = {{Little Kern River golden trout (Oncorhynchus mykiss whitei) naturally
   occur in high-elevation reaches of the Kern River drainage. Their
   environment is characterized by relatively low water temperatures and a
   short growing season (Knapp and Dudley 1990). Our study objective was to
   evaluate the physiological performance of juvenile golden trout (1.3 +/-
   0.02 g wet weight) under low to moderate water temperatures by measuring
   the effects of 10, 14, and 19degreesC water on their food consumption
   and growth rates, oxygen consumption rates, swimming performance, and
   acute upper thermal tolerance (critical thermal maxima, CTM).
   Differences among the food consumption rates were not statistically
   significant, but growth rates at 10degreesC (2.7 +/- 0.05 \% wt/d) were
   significantly lower than those at 14degreesC (3.3 +/- 0.14 \% wt/d) or
   19degreesC (3.4 +/- 0.16 \% wt/d). Oxygen consumption rates were
   significantly affected by fish size, but not by water temperature.
   Golden trout swimming at 19degreesC had significantly lower relative
   critical swimming velocities than those tested at 10 or 14degreesC.
   Critical thermal maxima was significantly affected by acclimation
   temperature, with a lower tolerance at 10degreesC (27.7 +/-
   0.12degreesC) than at 14 or 19degreesC (28.3 +/- 0.3degreesC and 28.4
   +/- 0.15degreesC, respectively). We conclude that Little Kern River
   golden trout are well-adapted to temperatures in the 10 to 19degreesC,
   although they show few indications of counter-gradient variation.}},
