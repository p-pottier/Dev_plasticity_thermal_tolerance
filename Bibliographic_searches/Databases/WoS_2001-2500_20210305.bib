@article{ ISI:000249608600009,
Author = {Tran, Dang Hoa and Takagi, Masami},
Title = {{Effects of low temperatures on pupal survival of the stone leek
   leafminer Liriomyza chinensis (Diptera : Agromyzidae)}},
Journal = {{INTERNATIONAL JOURNAL OF PEST MANAGEMENT}},
Year = {{2007}},
Volume = {{53}},
Number = {{3}},
Pages = {{253-257}},
Month = {{JUL-SEP}},
Abstract = {{The cold-hardiness of Liriomyza chinensis pupae was measured in the
   laboratory by observing pupal mortality at low temperatures. Pupal
   mortality increased with decreasing temperature and with extended cold
   exposure time. No recently pupated pupae (6 h) were able to survive
   after 16 days of chilling at 0, 2.5 and 5 degrees C, but 42.9\% survival
   was observed at 10 degrees C. Pupae at different developmental stages
   showed significant difference in mortality, with very low levels of
   mortality observed for older pupae (4 and 7 days) after exposure to 0
   degrees C for 16 days. The lethal time for 50\% survival (LT50)
   increased with increasing age of pupae. LT50 for 4-day-old pupae exposed
   to 0 degrees C was 52.1 days. Analysis of mean temperatures at several
   localities indicated that L. chinensis is able to overwinter outdoors in
   southern regions, but is unable to overwinter in open fields in northern
   regions of Japan, suggesting that overwintering in these regions would
   only occur in greenhouses.}},
