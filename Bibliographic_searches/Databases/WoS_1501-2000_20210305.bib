@article{ ISI:000291086800008,
Author = {Reyes, Izbelt and Diaz, Fernando and Denisse Re, Ana and Perez, Javier},
Title = {{Behavioral thermoregulation, temperature tolerance and oxygen
   consumption in the Mexican bullseye puffer fish, Sphoeroides annulatus
   Jenyns (1842), acclimated to different temperatures}},
Journal = {{JOURNAL OF THERMAL BIOLOGY}},
Year = {{2011}},
Volume = {{36}},
Number = {{3}},
Pages = {{200-205}},
Month = {{APR}},
Abstract = {{An inverse and unusual relationship was found between preferred
   temperature and acclimation temperature in the bullseye puffer,
   Sphoeroides annulatus. The final preferendum temperature (PT) was 26.8
   degrees C. The critical thermal maxima (CTMax) were 37.7, 38.8, 40.0,
   40.8 and 41.3 degrees C where the temperatures of acclimation were 19,
   22, 25, 28 and 31 degrees C +/- 1 degrees C, respectively, and the
   endpoint of CTMax was loss of the righting response. The acclimation
   response ratio presented an interval of 0.22-0.38: these values are in
   agreement with results for other subtropical and tropical fishes. The
   temperature significantly affected the oxygen consumption of bullseye
   puffer juveniles. The oxygen consumption rate (OCR) increased
   significantly with an increment in the temperature from 19 to 31 degrees
   C. The range of the temperature coefficient Q(10) in bullseye puffer
   individuals was lowest between 25 and 28 degrees C, at 1.37. The optimal
   temperature for growth was 26 degrees C. The results of this study will
   be useful for optimizing the culture of bullseye puffer juveniles in
   controlled conditions. (C) 2011 Elsevier Ltd. All rights reserved.}},
