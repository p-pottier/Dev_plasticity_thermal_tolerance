@article{ ISI:000289963500021,
Author = {Steinarsdottir, Maria Bjork and Ingolfsson, Agnar},
Title = {{METABOLIC ACTIVITY, REPRODUCTION AND SURVIVAL OF THE HIGH LITTORAL
   FRINGE COPEPOD ITUNELLA MUELLERI (HARPACTICOIDA: CANTHOCAMPTIDAE) AT
   DIFFERENT COMBINATIONS OF SALINITIES AND TEMPERATURES}},
Journal = {{JOURNAL OF CRUSTACEAN BIOLOGY}},
Year = {{2011}},
Volume = {{31}},
Number = {{2}},
Pages = {{364-366}},
Month = {{MAY}},
Abstract = {{Survival, metabolic and reproductive activity of Itunella muelleri was
   tested in several combinations of temperatures (2 degrees-30 degrees C)
   and salinities (from fresh water to salinity of 34 parts per thousand).
   Proportions of individuals alive after 18 or 22 days was high in all
   experimental combinations except in fresh water at 2 degrees C and at 30
   degrees C at salinities of 4 and 16 parts per thousand. Metabolic
   activity, as measured by the production of faecal pellets, was low in
   fresh water regardless of temperature. Metabolic activity was also very
   low at 2 degrees C regardless of salinity. Maximum activity was found at
   20 degrees C and 22 degrees C with little difference between salinities
   of 4, 8, 16, and 32 parts per thousand. Production of nauplii during the
   first 29 days was nil in fresh water regardless of temperature, and also
   nil at 2 degrees C and 30 degrees C regardless of salinities. The
   maximum production was found in the combination of 22 degrees C and
   salinity of 16 parts per thousand. The results are discussed in relation
   to expected ambient conditions in the natural habitat of the species.}},
