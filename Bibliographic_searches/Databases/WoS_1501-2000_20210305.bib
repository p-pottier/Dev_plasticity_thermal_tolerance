@article{ ISI:000267365000005,
Author = {Farcy, Emilie and Voiseux, Claire and Lebel, Jean-Marc and Fievet, Bruno},
Title = {{Transcriptional expression levels of cell stress marker genes in the
   Pacific oyster Crassostrea gigas exposed to acute thermal stress}},
Journal = {{CELL STRESS \& CHAPERONES}},
