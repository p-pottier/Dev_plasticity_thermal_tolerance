@article{ ISI:000302281100005,
Author = {Berman, D. I. and Leirikh, A. N. and Zhigul'skaya, Z. A.},
Title = {{A UNIFIED STRATEGY OF COLD HARDINESS IN ANTS OF THE GENUS MYRMICA
   (FORMICIDAE, HYMENOPTERA) IN NORTHEASTERN ASIA}},
Journal = {{ZOOLOGICHESKY ZHURNAL}},
Year = {{2012}},
Volume = {{91}},
Number = {{2}},
Pages = {{175-189}},
Month = {{FEB}},
Abstract = {{The biotopic distribution, arrangement of nests, wintering conditions
   and cold hardiness of four ecologically contrasting ant species (Myrmica
   angulinodis, M. kamtschatica, M. bicolor, and M. transsibirica) are
   considered. The cold hardiness of these species is typical for the
   species of the genus: supercooling points are -28...-31 degrees C; cold
   hardiness levels (LT50\%) are higher by 5 degrees C. Having such a level
   of cold hardiness, ants can inhabit everywhere in the Hypoarctic.
   However, the Myrmica species studied are distinctly segregated (M.
   kamtschatica occupies moss bogs; M. angulinodis and M. transsibirica
   inhabit dry and warm southern slopes; M. bicolor, sandy-gravel
   floodplains) due to their different demands to summer conditions.
   Nowadays, the higher (excess) cold hardiness (by 5-10 degrees C
   regarding temperatures in nests) is inadaptive and can be considered as
   pre-adaptive. This excess cold hardiness could be formed in ancient
   cooling periods or was assumed as an accompanying property in the course
   of adaptation not to low temperatures, but to some other factors-for
   instance, to droughts. The selection of the Myrmica species to resist
   negative temperatures does not work, since the present cold hardiness is
   characterized by temperatures that are much lower than the possible
   temperature of wintering.}},
