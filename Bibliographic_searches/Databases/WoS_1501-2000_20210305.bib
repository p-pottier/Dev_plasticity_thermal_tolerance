@article{ ISI:000298051200009,
Author = {Dierks, Anneke and Hoffmann, Bianca and Bauerfeind, Stephanie S. and
   Fischer, Klaus},
Title = {{Effects of inbreeding on life history and thermal performance in the
   tropical butterfly Bicyclus anynana}},
Journal = {{POPULATION ECOLOGY}},
Year = {{2012}},
Volume = {{54}},
Number = {{1}},
Pages = {{83-90}},
Month = {{JAN}},
Abstract = {{Human-induced loss and fragmentation of natural habitats reduces
   population size and thereby presumably genetic diversity through
   inbreeding or genetic drift. Additionally, many species are confronted
   with increased temperature stress due to climate change, with reduced
   genetic diversity potentially interfering with a species' ability to
   cope with such conditions. While in general the detrimental impact of
   inbreeding has often been documented, its consequences for the ability
   to cope with temperature stress are still poorly understood. Against
   this background we here investigate the effects of inbreeding on egg
   hatching success, development and temperature stress tolerance in the
   tropical butterfly Bicyclus anynana. Specifically we test for an
   increased sensitivity to environmental stress in inbred individuals. Our
   results revealed that even comparatively low levels of inbreeding yield
   negative consequences for reproduction and development under beneficial
   conditions. Inbreeding also reduced cold tolerance in adult butterflies,
   while heat tolerance remained unaffected. We therefore conclude that
   acute stress tolerance may not be generally impaired by inbreeding.}},
