@article{ ISI:000267453400006,
Author = {Wang, S. and Johnson, J. A. and Hansen, J. D. and Tang, J.},
Title = {{Determining thermotolerance of fifth-instar Cydia pomonella (L.)
   (Lepidoptera: Tortricidae) and Amyelois transitella (Walker)
   (Lepidoptera: Pyralidae) by three different methods}},
Journal = {{JOURNAL OF STORED PRODUCTS RESEARCH}},
