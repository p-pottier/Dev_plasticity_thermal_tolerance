@article{ ISI:000254088900014,
Author = {Mather, Martha E. and Parrish, Donna L. and Campbell, Cara A. and
   McMenemy, James R. and Smith, Joseph M.},
Title = {{Summer temperature variation and implications for juvenile Atlantic
   salmon}},
