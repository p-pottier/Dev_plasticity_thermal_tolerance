@article{ ISI:000303489500001,
Author = {Campos, Joana and Moreira, Claudia and Freitas, Fabiana and van der
   Veer, Henk W.},
Title = {{SHORT REVIEW OF THE ECO-GEOGRAPHY OF CRANGON}},
Journal = {{JOURNAL OF CRUSTACEAN BIOLOGY}},
