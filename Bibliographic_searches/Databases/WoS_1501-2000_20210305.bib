@inproceedings{ ISI:000395947900052,
Author = {Dautel, Hans and Knuelle, Wilhelm},
Editor = {{Sabelis, MW and Bruin, J}},
Title = {{Embryonic diapause and cold hardiness of lxodes ricinus eggs (Acari:
   Ixodidae)}},
Booktitle = {{TRENDS IN ACAROLOGY}},
Year = {{2010}},
Pages = {{327-331}},
Note = {{12th International Congress of Acarology - Trends in Acarology,
   Amsterdam, NETHERLANDS, AUG 21-26, 2006}},
Organization = {{Koppert Biol Syst; Nethorlands Genom Inst; Tech Ctr Agr \& Rural
   Cooperat; Indira Publish House; City Council Amsterdam; Inst Biodivers
   \& Ecosyst Dynam}},
Abstract = {{Ixodes ricinus is medically the most important tick species in temperate
   Europe. The present study was undertaken to assess the effect of
   photoperiod and low-temperature exposure on termination of embryonic
   diapause and on cold hardiness in I. ricinus eggs. Engorged I. ricinus
   females (n = 9) were kept at 90\% r.h., 15 degrees C longday (L17:D7),
   or 15 degrees C shortday (L14:D10), or at an outdoor site providing
   natural temperatures and daylength. Eggs of defined ages were
   continuously kept under these conditions or subjected to changes in
   photoperiod and temperature, and the effect on diapause incidence was
   observed. At 15 degrees C, non-diapause and diapause eggs hatched after
   70-120 and 140-240 days, respectively. Interestingly, most egg masses
   did not show an all or nothing reaction but a certain percentage of
   diapause eggs. Cold exposure of eggs to 4 degrees C for 6 weeks
   completely terminated diapause, whereas the effect of photoperiod was
   negligible. Also exposure to temperatures between -10 and -20 degrees C
   for 24 h terminated diapause. At the outdoor site, eggs were laid in
   winter, from mid October to early April. All eggs hatched between mid
   June and early July. The supercooling point of these eggs was around -28
   degrees C between November and January and rose to -27 degrees C in
   April. At constant 15 degrees C, the supercooling point rose from -30
   degrees C in young eggs to -24 degrees C in physiologically older eggs.
   The lower lethal temperature in diapause and non-diapause eggs was -21.6
   and -18.0 degrees C, respectively, but the difference was not
   significant.}},
