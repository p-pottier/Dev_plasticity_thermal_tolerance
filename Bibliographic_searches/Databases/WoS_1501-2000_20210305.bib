@article{ ISI:000278355800004,
Author = {Voutilainen, A. and Taskinen, J. and Huuskonen, H.},
Title = {{Temperature-dependent effect of the trematode eye flukes Diplostomum
   spp. on the growth of Arctic charr Salvelinus alpinus (L.)}},
Journal = {{BULLETIN OF THE EUROPEAN ASSOCIATION OF FISH PATHOLOGISTS}},
Year = {{2010}},
Volume = {{30}},
Number = {{3}},
Pages = {{106-113}},
Abstract = {{The Diplostomum spp. infected juvenile Arctic charr reared at 9.5
   degrees C had lower specific growth rates than the unexposed control
   fish reared at the same temperature. At the temperature optimal for the
   growth of juvenile Arctic charr, 14.5 degrees C, no difference in the
   growth between the infected and control fish was observed. At 14.5
   degrees C Diplostomum spp. larvae located in the fish eye lenses grew
   and developed into metacercariae in 48 days, whereas at 9.5 degrees C
   the parasites did not grow at all, although they maintained they
   capability to move. The negative effect of Diplostomum spp. on the
   growth of their fish host at low temperatures (<10 degrees C) is
   especially significant in the case of Arctic charr, which is adapted to
   live in a cold environment. It was suggested that Diplostomum spp. may
   affect the energy metabolism of their fish hosts especially when the
   fish have less energetic capacity left to compensate the reduced growth,
   i.e. at temperatures close to the lower or upper thermal limits for the
   growth of the fish.}},
