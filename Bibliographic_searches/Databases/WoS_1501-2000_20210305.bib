@article{ ISI:000276607700007,
Author = {Chown, Steven L. and Gaston, Kevin J. and van Kleunen, Mark and
   Clusella-Trullas, Susana},
Title = {{Population responses within a landscape matrix: a macrophysiological
   approach to understanding climate change impacts}},
