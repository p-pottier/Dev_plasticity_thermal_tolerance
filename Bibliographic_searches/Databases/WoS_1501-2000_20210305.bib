@article{ ISI:000281054300001,
Author = {Sosalegowda, Aparna H. and Kundapur, Rajesh R. and Boregowda, Manjunatha
   H.},
Title = {{Molecular characterization of heat shock proteins 90 (HSP83?) and 70 in
   tropical strains of Bombyx mori}},
Journal = {{PROTEOMICS}},
