@inproceedings{ ISI:000278474900067,
Author = {Cunningham, A. W. and Watts, S. A.},
Editor = {{Harris, LG and Bottger, SA and Walker, CW and Lesser, MP}},
Title = {{The effect of temperature on early embryonic development in Lytechinus
   variegatus embryos from adults with different thermal histories}},
Booktitle = {{ECHINODERMS: DURHAM}},
Year = {{2010}},
Pages = {{451-455}},
Note = {{12th International Echinoderm Conference, Univ New Hampshire, Durham,
   NH, AUG 07-11, 2006}},
Abstract = {{Thermal effects on early embryonic development varied with the thermal
   history of adult Lytechinus variegatus. The thermal tolerances of
   embryos from adults held at 18 or 28 degrees C for 10-12 weeks were
   compared. Thermal tolerances of embryos from adults collected in the
   spring or the summer were also compared. The strategies of the spring
   and summer embryos roughly paral- leled that of the 18 C and 28 degrees
   C treatments, respectively. The results for the 18 and 28 degrees C
   treatments and the spring and summer treatments suggest that eggs
   produced by adults with thermal histories resembling summer temperatures
   in St. Joseph Bay are better adapted to summer temperatures, but are
   generally less likely to be viable than eggs from adults with thermal
   histories resembling spring temperatures in the field.}},
