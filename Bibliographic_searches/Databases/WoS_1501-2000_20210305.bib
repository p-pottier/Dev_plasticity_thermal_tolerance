@article{ ISI:000298451200042,
Author = {Sandhu, Hardev S. and Shi, Peijian and Kuang, Xianju and Xue, Fangsen
   and Ge, Feng},
Title = {{APPLICATIONS OF THE BOOTSTRAP TO INSECT PHYSIOLOGY}},
