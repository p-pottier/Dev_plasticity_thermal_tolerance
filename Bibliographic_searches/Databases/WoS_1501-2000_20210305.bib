@article{ ISI:000285419300001,
Author = {Righton, David A. and Andersen, Ken Haste and Neat, Francis and
   Thorsteinsson, Vilhjalmur and Steingrund, Petur and Svedang, Henrik and
   Michalsen, Kathrine and Hinrichsen, Hans-Harald and Bendall, Victoria
