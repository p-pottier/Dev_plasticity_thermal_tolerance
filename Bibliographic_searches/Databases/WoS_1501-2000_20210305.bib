@article{ ISI:000273272600012,
Author = {Xu, Yi-Juan and Lu, Yong-Yue and Pan, Zhi-Ping and Zeng, Ling and Liang,
   Guang-Wen},
Title = {{Heat Tolerance of the Red Imported Fire Ant, Solenopsis invicta
   (Hymenoptera: Formicidae) in Mainland China}},
Journal = {{SOCIOBIOLOGY}},
Year = {{2009}},
Volume = {{54}},
Number = {{1}},
Pages = {{115-126}},
Abstract = {{This study assessed the effects of high temperature on the survival of
   the fire ant in the laboratory. Workers were placed in incubators
   ofvaried temperatures (40 degrees C, 40-5 degrees C, 41 degrees C, 41.5
   degrees C, 42.0 degrees C, 42.5 degrees C, 43.0 degrees C, 44.5 degrees
   C and 45 degrees C), and the number ofthe dead and surviving workers
   were counted and recorded after 0.5, 1, 2, 4, 6, 8, 10 and 12 h. The
   results showed that the susceptible lethal ternperature of the fire ant
   was higher than 41.48 degrees C when exposure time was no longer than 4
   h. The survival variation of the fire ant workers was regular when they
   were exposed to different high temperatures in a certain range of time,
   i.e. there was a close relationship between the temperatures and the
   number of surviving workers, as the Logistic equation described. Under
   the heat stress, regression of exposure time and survival rate of the
   workers was consistent with Logistic and Weibull distribution equations.
   Heat tolerance of the fire ant workers could be improved if they were
   pre-exposed to non-lethal high temperatures for a short period of time,
   and exposure to 36 degrees C for 1h was the optimum condition for heat
   acclimation ofthe fire ant, with the survival rate as high as 68\%. In
   contrast, the survival rate was just 36.7\% for those untreated workers.}},
