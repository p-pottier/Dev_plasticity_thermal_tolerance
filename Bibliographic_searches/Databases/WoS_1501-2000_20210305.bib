@article{ ISI:000260616400008,
Author = {Whalan, S. and Ettinger-Epstein, P. and de Nys, R.},
Title = {{The effect of temperature on larval pre-settlement duration and
   metamorphosis for the sponge, Rhopaloeides odorabile}},
Journal = {{CORAL REEFS}},
Year = {{2008}},
Volume = {{27}},
Number = {{4}},
Pages = {{783-786}},
Month = {{DEC}},
Abstract = {{Rising sea temperatures may potentially affect the dispersive larval
   phase of sessile marine invertebrates with consequences for the
   viability of adult populations. This study demonstrated that the
   planktonic larvae of Rhopaloeides odorabile, a common Great Barrier Reef
   sponge, survived and metamorphosed when exposed to temperatures up to 9
   degrees C above the annual maximum (similar to 29 degrees C). Planktonic
   larval duration of 54 h, at ambient temperatures (similar to 28 degrees
   C), were reduced to 18 h for larvae exposed to elevated temperatures
   (32-36 degrees C). Moreover, at ambient temperatures larvae began
   metamorphosing after 12 h, but at 32-36 degrees C this reduced to only 2
   h. Larvae survived and could still metamorphose at temperatures as high
   as 38 degrees C, but were no longer functional at 40 degrees C. These
   results imply that predicted increases in sea surface temperature may
   reduce planktonic larval duration and dispersal capabilities, thereby
   contributing to population subdivision of the species.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
