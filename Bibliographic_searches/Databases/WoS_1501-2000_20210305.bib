@article{ ISI:000290269900004,
Author = {Pappas, M. L. and Broufas, G. D. and Koufali, N. and Pieri, P. and
   Koveos, D. S.},
Title = {{Effect of heat stress on survival and reproduction of the olive fruit
   fly Bactocera (Dacus) oleae}},
Journal = {{JOURNAL OF APPLIED ENTOMOLOGY}},
Year = {{2011}},
Volume = {{135}},
Number = {{5}},
Pages = {{359-366}},
Month = {{JUN}},
Abstract = {{The olive fruit fly Bactrocera (Dacus) oleae Gmelin is a major olive
   pest in Greece and other Mediterranean countries. Its population density
   and respective olive infestation is usually low in many areas of
   northern Greece during summer months. To some extent, this may be due to
   the prevailing high temperature and low relative humidity conditions. In
   the present work the effects of short term exposure to high temperatures
   on the survival and egg production of B. oleae pre-imaginal stages and
   adults were studied under laboratory conditions. Different larval
   instars within infested green olive fruits, adults and pupae and were
   exposed for 2 h to a series of different high constant temperatures
   ranging from 34 to 42 degrees C. Subsequently, survival percentages of
   pre-imaginal stages and adults as well as the number of eggs laid by
   females previously exposed to high temperatures were determined. At
   temperatures up to 38 degrees C high survival percentages of larvae and
   adults were observed, whereas pupae displayed a relatively increased
   heat tolerance up to 40 degrees C. Female longevity and egg production
   were substantially reduced after heat stress. Prior acclimation at 33
   degrees C for 1 and 3 days resulted in increased adult survival
   following heat stress. We discuss the results with respect to the
   ability of the fly to survive and reproduce under high summer
   temperatures.}},
