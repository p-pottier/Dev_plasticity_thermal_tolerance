@article{ ISI:000257550100010,
Author = {Kir, Mehmet and Kumlu, Metin},
Title = {{Effect of temperature and salinity on low thermal tolerance of Penaeus
   semisulcatus (Decapoda : Penaeidae)}},
Journal = {{AQUACULTURE RESEARCH}},
Year = {{2008}},
Volume = {{39}},
Number = {{10}},
Pages = {{1101-1106}},
Month = {{JUL 17}},
Abstract = {{Critical thermal minima (CTmin) and lower incipient lethal temperature
   (LILT) were determined for Penaeus semisulcatus juveniles at
   combinations of four acclimation temperatures (18, 22, 26 and 30 degrees
   C) and four salinity levels (20, 25, 30 and 35 ppt). While the CTmin and
   LILT of shrimps were not affected significantly by salinity (P > 0.05),
   a direct relationship was determined between the thermal minima values
   (CTmin, LILT) and the acclimation temperature (P < 0.05). The
   acclimation response ratio for the juveniles of P. semisulcatus had an
   interval of 0.18-0.32 in the current study.}},
