@article{ ISI:000268150600012,
Author = {Weiss, Monika and Thatje, Sven and Heilmayer, Olaf and Anger, Klaus and
   Brey, Thomas and Keller, Martina},
Title = {{Influence of temperature on the larval development of the edible crab,
   Cancer pagurus}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
Year = {{2009}},
Volume = {{89}},
Number = {{4}},
Pages = {{753-759}},
Month = {{JUN}},
Abstract = {{The influence of temperature on larval survival and development was
   studied in the edible crab, Cancer pagurus, from a population off the
   island of Helgoland, North Sea. In rearing experiments conducted at six
   different temperatures (6 degrees, 10 degrees, 14 degrees, 15 degrees,
   18 degrees and 24 degrees C), zoeal development was only completed at 14
   degrees and 15 degrees C. Instar duration of the Zoea I was negatively
   correlated with temperature. A model relating larval body mass to
   temperature and developmental time suggests that successful larval
   development is possible within a narrow temperature range (14 degrees
   +/- 3 degrees C) only. This temperature optimum coincides with the
   highest citrate synthase activity found at 14 degrees C. A comparison
   for intraspecific variability among freshly hatched zoeae from different
   females (CW 13 -17 cm, N = 8) revealed that both body mass and elemental
   composition varied significantly. Initial larval dry weight ranged from
   12.1 to 17.9 mu g/individual, the carbon content from 4.6 to 5.8 mu
   g/individual, nitrogen from 1.1 to 1.3 mu g/individual, and the C:N
   ratio from 4.1 to 4.4. A narrow larval temperature tolerance range of C.
   pagurus as well as the indication of intraspecific variability in female
   energy allocation into eggs may indicate a potential vulnerability of
   this species to climate change. Large-scale studies on the ecological
   and physiological resilience potential of this commercially fished
   predator are needed.}},
