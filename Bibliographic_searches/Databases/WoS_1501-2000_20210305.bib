@article{ ISI:000255811500032,
Author = {Klose, M. K. and Atwood, H. L. and Robertson, R. M.},
Title = {{Hyperthermic preconditioning of presynaptic calcium regulation in
   Drosophila}},
Journal = {{JOURNAL OF NEUROPHYSIOLOGY}},
Year = {{2008}},
Volume = {{99}},
Number = {{5}},
Pages = {{2420-2430}},
Month = {{MAY}},
Abstract = {{We examined the thermosensitivity of calcium regulation in Drosophila
   larval neuromuscular junctions, testing effects of prior heat shock and
   Hsp70 expression. Motor neurons were loaded with either the ratiometric
   indicator Fura-dextran or the nonratiometric indicator Oregon Green
   bis-(o-aminophenoxy)-N,N,N',N'-tetraacetic acid to monitor parameters of
   calcium regulation as temperature increased. Nerve terminals treated to
   a prior heat shock, and those of transgenic flies expressing higher than
   normal levels of Hsp70, were better able to maintain near-normal resting
   calcium concentrations, calcium influx, and calcium clearance at higher
   temperatures. Synaptic transmission was also protected by prior heat
   shock and by higher than normal Hsp70 expression. Thus the thermal limit
   of synaptic transmission may be directly linked to the stability of
   calcium regulation.}},
